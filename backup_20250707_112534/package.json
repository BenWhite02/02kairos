    "workbox-window": "^7.3.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check && npm run test"
    }
  },
  "keywords": [
    "kairos",
    "marketing",
    "decisioning",
    "eligibilityatoms",
    "moments",
    "campaigns",
    "react",
    "typescript",
    "vite",
    "tailwindcss"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/BenWhite02/02kairos.git"
  },
  "bugs": {
    "url": "https://github.com/BenWhite02/02kairos/issues"
  },
  "homepage": "https://github.com/BenWhite02/02kairos#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "volta": {
    "node": "20.18.0",
    "npm": "10.8.2"
  }
