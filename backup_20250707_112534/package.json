    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.0",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.60.0",
    "react-router-dom": "^7.6.3",
    "zod": "^3.25.75",
    "zustand": "^5.0.6",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "recharts": "^2.13.3",
    "react-chartjs-2": "^5.2.0",
    "chart.js": "^4.4.6",
    "d3": "^7.9.0",
    "@types/d3": "^7.4.3",
    "lodash": "^4.17.21",
    "@types/lodash": "^4.17.15",
    "uuid": "^11.0.3",
    "@types/uuid": "^11.0.3",
    "immer": "^10.1.1",
